$('#member-pagination').html('<%= escape_javascript(paginate @users, param_name: :member_page, params_on_first_page: true, remote: true ) %>');
$('#member-table').html('<%= j render partial: 'admin/member_table_partial', locals: {users: @users} %>');
$('#group-pagination').html('<%= escape_javascript(paginate @groups, param_name: :group_page, params_on_first_page: true, remote: true ) %>');
$('#group-table').html('<%= j render partial: 'admin/group_table_partial', locals: {groups: @groups} %>');
$('#flash-messages').html('<%= j render partial: 'shared/flash_messages', flash: flash %>');